{
  "name": "rsoeta/pakenjeng-tangguh",
  "description": "SINDEN DTSEN CI4 project for PHP 8.1 hosting",
  "require": {
    "php": ">=8.1",
    "codeigniter4/framework": "^4.6.2",
    "kint-php/kint": "5.0",
    "phpoffice/phpspreadsheet": "*"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "platform": {
      "php": "8.1.0"
    },
     "audit": {
        "block-insecure": false
    }
  },
  "scripts": {
    "post-update-cmd": [
      "CodeIgniter\\ComposerScripts::postUpdate",
      "php spark commands"
    ],
    "post-install-cmd": [
    ],
    "auto-fix-kint": "php scripts/fix_kint.php"
  }
}
